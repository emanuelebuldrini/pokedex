{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Pokeapi": {
    "BaseUrl": "https://pokeapi.co/api/v2",
    "CacheDuration": "1.00:00:00"
  },
  "FuntranslationsApi": {
    "BaseUrl": "https://api.funtranslations.com/translate",
    "CacheDuration": "1.00:00:00"
  },
  "RetryPolicy": {
    "BackoffStrategy": "Exponential",
    "RetryCount": 3,
    "BaseDelaySeconds": 2
  }
}
